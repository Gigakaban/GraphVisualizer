<Window x:Class="GraphProgram.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphProgram"
        mc:Ignorable="d"
        Title="Dynamic Graph" Height="450" Width="800" Icon="unknown_2.png">
    <Canvas x:Name="MainCanvas" Background="LightGray">
        <Button Content="Задати" Canvas.Left="80" Canvas.Top="38" Click="Button_Click" HorizontalAlignment="Left" VerticalAlignment="Center" Width="48"/>
        <Label Content="n =" Canvas.Left="10" Canvas.Top="35" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <TextBox x:Name="CountOfV" Canvas.Left="38" TextWrapping="Wrap" Text="10" Canvas.Top="39" Width="20" HorizontalAlignment="Left" VerticalAlignment="Center" Height="18" />
    </Canvas>


</Window>
